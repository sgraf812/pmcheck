%include polycode.fmt

% suppress some built-in formatting of lhs2TeX:
%format not = "\id{not}"
%format == = "\mathop{==}"
%format /= = "\mathop{{/}{=}}"
%format && = "\mathop{\&\&}"
%format !! = "\;!"
%format !_ = "\;!\_"
%format |> = "\triangleright"
%format corefltau = "\corefl{\tau}"
%format conewt = "\conewt"
%format cosym = "\cosym"
%format coseq = "\coseq"

